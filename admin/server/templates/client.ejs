<!DOCTYPE html>
<html class="" <% if(htmlWebpackPlugin.files.manifest) { %> manifest="
<%= htmlWebpackPlugin.files.manifest %>"
<% } %>>

<head>
  <meta charset="utf-8">
  <meta name="x5-fullscreen" content="true">
  <meta name="full-screen" content="yes">
  <% if (__LANG__ === 'en') { %>
  <link rel="dns-prefetch" href="//assets.veervr.tv" />
  <link rel="dns-prefetch" href="//d2df2pjg8l347k.cloudfront.net" />
  <link rel="dns-prefetch" href="//api.veer.tv" />
  <link rel="dns-prefetch" href="//d147wy1xnm9vni.cloudfront.net" />
  <% } else { %>
  <link rel="dns-prefetch" href="//assets.veervr.tv" />
  <link rel="dns-prefetch" href="//api.veervr.tv" />
  <link rel="dns-prefetch" href="//qcdn.veervr.tv" />
  <% } %>
  <link rel="manifest" href="/manifest.json">
  <title>
    <%= htmlWebpackPlugin.options.title || 'VeeR VR'%>
  </title>

  <% if (htmlWebpackPlugin.options.keywords) { %>
  <meta name="keywords" content="<%= htmlWebpackPlugin.options.keywords %>" />
  <% } %>

  <% if (htmlWebpackPlugin.options.description) { %>
  <meta name="description" content="<%= htmlWebpackPlugin.options.description %>" />
  <% } %>

  <% if (__LANG__ === 'en') { %>
  <meta name="msvalidate.01" content="2C278935FD4407685AB136118D715BBA" />
  <% } else { %>
  <meta name="shenma-site-verification" content="28e671d951623543b7b39c9848b8d981_1533279029" />
  <% } %>

  <% if (__LANG__ === 'en') { %>
  <link rel="shortcut icon" href="https://d2df2pjg8l347k.cloudfront.net/www-app/favicon_72cade8914acc7de84197b2b8ce38ec0.png">
  <% } else { %>
  <link rel="shortcut icon" href="https://assets.veervr.tv/www-app/favicon_72cade8914acc7de84197b2b8ce38ec0.png">
  <% } %>

  <% if (htmlWebpackPlugin.options.mobile) { %>
  <meta name="viewport" content="width=device-width, initial-scale=1.0, minimum-scale=1.0, maximum-scale=1.0, user-scalable=no" />
  <meta name="apple-itunes-app" content="app-id=1140023495" />
  <% } %>
  <style>
    html,body{height: 100%; background-color: #000; padding: 0;margin: 0;}
      #app,.wrapper {position: relative;height: 100%;}
      #app .placeholder-contents {display: flex; height: 100%; flex-direction: column; justify-content: center; align-items: center;}
      #app ._desc{margin-top: 20px;}
      #app .placeholder-contents p{color:#888; font-size: 12px;}
  </style>
  <% if (htmlWebpackPlugin.options.baseHref) { %>
  <base href="<%= htmlWebpackPlugin.options.baseHref %>" />
  <% } %>
</head>

<body>
  <% if (htmlWebpackPlugin.options.unsupportedBrowser) { %>
  <style>.unsupported-browser { display: none; }</style>
  <div class="unsupported-browser">
    Sorry, your browser is not supported. Please upgrade to
    the latest version or switch your browser to use this site.
    See <a href="http://outdatedbrowser.com/">outdatedbrowser.com</a>
    for options.
  </div>
  <% } %>

  <% if (htmlWebpackPlugin.options.appMountId) { %>
  <div id="<%= htmlWebpackPlugin.options.appMountId%>">
    <div class="placeholder-contents">
      <img alt="veer vr" width="50" src="https://assets.veervr.tv/@vrups/66/45b092e1fd31df-100-100.png" />
      <img class="_desc" height="24" alt="veer contents" src="https://assets.veervr.tv/@vrups/87/c2353c1edf34d0-215-60.png" />
      <p>Global VR Content Community</p>
    </div>
  </div>
  <% } %>

  <% if (htmlWebpackPlugin.options.appMountIds && htmlWebpackPlugin.options.appMountIds.length > 0) { %>
  <% for (var index in htmlWebpackPlugin.options.appMountIds) { %>
  <div id="<%= htmlWebpackPlugin.options.appMountIds[index]%>"></div>
  <% } %>
  <% } %>

  <% if (htmlWebpackPlugin.options.window) { %>
  <script>
  <% for (var varName in htmlWebpackPlugin.options.window) { %>
      window['<%=varName%>'] = <%= JSON.stringify(htmlWebpackPlugin.options.window[varName]) %>;
  <% } %>
  </script>
  <% } %>

  <% if (isProd) { %>
    <% if (__LANG__ === 'en') { %>
      <script>
       (function (i, s, o, g, r, a, m) {
         i['GoogleAnalyticsObject'] = r; i[r] = i[r] || function () {
           (i[r].q = i[r].q || []).push(arguments)
         }, i[r].l = 1 * new Date(); a = s.createElement(o),
         m = s.getElementsByTagName(o)[0]; a.async = 1; a.src = g; m.parentNode.insertBefore(a, m)
       })(window, document, 'script', 'https://www.google-analytics.com/analytics.js', 'ga');

       ga('create', 'UA-88657878-1', 'auto');
      </script>

      <script async type="text/javascript" src="//www.googleadservices.com/pagead/conversion_async.js">
      </script>
      <!-- Global site tag (gtag.js) - Google AdWords: 862692150 -->
      <script async src="https://www.googletagmanager.com/gtag/js?id=AW-862692150"></script>
      <script src="//cdn.optimizely.com/js/10239430432.js"></script>
      <script>
       window.dataLayer = window.dataLayer || [];
       function gtag() { dataLayer.push(arguments); }
       gtag('js', new Date());

       gtag('config', 'AW-862692150');
      </script>
      <!-- Quora Pixel Code (JS Helper) -->
      <script>
       !function (q, e, v, n, t, s) {
         if (q.qp) return; n = q.qp = function () { n.qp ? n.qp.apply(n, arguments) : n.queue.push(arguments); }
         ; n.queue = []; t = document.createElement(e); t.async = !0; t.src = v; s = document.getElementsByTagName(e)[0]; s.parentNode.insertBefore(t, s);
       }(window, 'script', 'https://a.quora.com/qevents.js');
       qp('init', 'c0da49af6ed345a796c290e76c80c694');
       qp('track', 'ViewContent');
      </script>
      <noscript>
        <img height="1" width="1" style="display:none" src="https://q.quora.com/_/ad/c0da49af6ed345a796c290e76c80c694/pixel?tag=ViewContent&noscript=1" />
      </noscript>
      <!-- End of Quora Pixel Code -->
    <% } else { %>
      <script>
       var _hmt = _hmt || [];
       (function () {
         var hm = document.createElement("script");
         hm.crossorigin = "anonymous";
         hm.src = "https://hm.baidu.com/hm.js?58dd6793fd10e9c64f5aa81e3b160aa0";
         var s = document.getElementsByTagName("script")[0];
         s.parentNode.insertBefore(hm, s);
       })();
       (function () {
         var bp = document.createElement('script');
         bp.crossorigin = "anonymous";
         bp.async = true;
         var curProtocol = window.location.protocol.split(':')[0];
         if (curProtocol === 'https') {
           bp.src = 'https://zz.bdstatic.com/linksubmit/push.js';
         }
         else {
           bp.src = 'http://push.zhanzhang.baidu.com/push.js';
         }
         var s = document.getElementsByTagName("script")[0];
         s.parentNode.insertBefore(bp, s);
       })();
      </script>
    <% } %>
  <% } %>
  <script>
   // add wehcat sdk
   (function () {
     var ua = navigator.userAgent.toLowerCase();

     if ((/MicroMessenger/i.test(ua) || /MQQBrowser/i.test(ua))) {
       var script = document.createElement('script');
       script.async = true;
       script.src = '//res.wx.qq.com/open/js/jweixin-1.3.2.js';
       document.body.appendChild(script);
     }
   })();
  </script>
</body>
</html>
